  "keywords": [
    "Terra",
    "Health",
    "Fitness",
    "Dashboard",
    "Graphs"
  ],
  "author": "JaafarRammal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tryterra/terra-graphs/issues"
  },
  "homepage": "https://github.com/tryterra/terra-graphs#readme",
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@types/styled-components": "^5.1.25",
    "prettier": "^2.7.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  }
}
