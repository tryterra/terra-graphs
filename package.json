{
  "name": "terra-graphs",
  "version": "1.0.1",
  "description": "Wrapper for the terra graphs API",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "exit 0",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
